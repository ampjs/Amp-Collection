{
  "name": "@ampersarnie/collection",
  "version": "0.7.0",
  "description": "Collection",
  "main": "collection.js",
  "options": {},
  "scripts": {
    "mocha": "./node_modules/.bin/mocha --growl test/tests.js --compilers js:babel-core/register -r jsdom-global/register",
    "mocha:report": "npm run mocha -- --reporter markdown test/tests.js > test/REPORT.md; echo $(($(date +'%s * 1000 + %-N / 1000000'))) >> test/REPORT.md",
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "build:test": "npm run build; npm run mocha",
    "prepublish": "npm run build; npm run mocha; npm run mocha:report",
    "lint": "eslint src/*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "react",
    "javascript"
  ],
  "author": "Paul Taylor <hello@ampersandwich.co.uk> (http://ampersandwich.co.uk)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "mocha": "^2.4.5",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "^0.1.4"
  },
  "dependencies": {
    "@ampersarnie/implements": "^0.3.1",
    "babel-runtime": "^6.23.0"
  }
}

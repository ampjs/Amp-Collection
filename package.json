{
  "name": "@ampersarnie/collection",
  "version": "0.5.0",
  "description": "Collection",
  "main": "collection.js",
  "options": {},
  "scripts": {
    "mocha": "node_modules/.bin/mocha --growl test/tests.js",
    "mocha:report": "node_modules/.bin/mocha --reporter markdown test/tests.js > test/RESULTS.md; echo $(($(date +'%s * 1000 + %-N / 1000000'))) >> test/RESULTS.md",
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "build:test": "npm run compile; npm run mocha",
    "prepublish": "npm run compile; npm run mocha; npm run mocha:report",
    "lint": "eslint src/*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "react",
    "javascript"
  ],
  "author": "Paul Taylor <hello@ampersandwich.co.uk> (http://ampersandwich.co.uk)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "@ampersarnie/implements": "^0.2.0"
  }
}
